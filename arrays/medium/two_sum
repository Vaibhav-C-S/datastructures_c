class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
    """
        
        dict={}
        for i,num in enumerate(nums):
            com = target - num
            if com not in dict:
                dict[num] = i
            else:
                return [dict[com],i]
        return []

time = o(n):n is size of array
